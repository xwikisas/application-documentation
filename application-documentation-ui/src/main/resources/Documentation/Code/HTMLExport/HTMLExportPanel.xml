#panelheader($services.localization.render('documentation.panels.htmlExport.title'))
## Generate the export URL : exports everything within the Documentation space
#set($wikiID = $services.wiki.currentWikiId)
#set($exportURL = $xwiki.getURL('Documentation.WebHome', 'export', "format=html&amp;pages=${wikiID}:Documentation%25&amp;skin=Documentation.Code.HTMLExport.HTMLExportSkin&amp;showShortcuts=1"))
#set($discard = $xwiki.ssx.use('Documentation.Code.HTMLExport.HTMLExportPanel'))
{{html clean="false"}}
  &lt;a href="$escapetool.xml($exportURL)" id="documentationExportButton" class="button" target="_blank"&gt;$escapetool.xml($services.localization.render('documentation.panels.htmlExport.export'))&lt;/a&gt;
{{/html}}
#panelfooter()
{{/velocity}}</content>
