    </property>
    <property>
      <code>require(['jquery'], function($) {
  let documentationTreePanel = $('.DocumentationTreePanel');
  let panelHeader = documentationTreePanel.find('.xwikipaneltitle');
  let button = $(
    `&lt;a name="documentation-toc-expand-collapse" class="btn btn-default btn-documentation-toc-expand-collapse" data-expanded="false"&gt;
      &lt;span class="fa fa-caret-down action-expand"&gt;&lt;/span&gt;
      &lt;span class="fa fa-caret-up action-collapse"&gt;&lt;/span&gt;
    &lt;/a&gt;`);
  button.insertBefore(panelHeader);

  $('.btn-documentation-toc-expand-collapse').on('click', function() {
    let expanded = $(this).attr('data-expanded') === 'true';
    let xtree = documentationTreePanel.find('.xtree');

    if (!expanded) {
      xtree.jstree('open_all');
      $(this).attr('data-expanded', 'true');
    } else {
      xtree.jstree('close_all');
      $(this).attr('data-expanded', 'false');
    }
  });
})</code>
