    <property>
      <code>.DocumentationTreePanel {
  position: relative;

  .flat-buttons();

  .btn-documentation-toc-expand-collapse {
    background: white;
    position: absolute;
    right: 15px;
    top: 0;
    padding: 2px 8px;

    &amp;[data-expanded="false"] {
      .action-expand {
        display: inline;
      }

      .action-collapse {
        display: none;
      }
    }

    &amp;[data-expanded="true"] {
      .action-expand {
        display: none;
      }

      .action-collapse {
        display: inline;
      }
    }
  }
}

